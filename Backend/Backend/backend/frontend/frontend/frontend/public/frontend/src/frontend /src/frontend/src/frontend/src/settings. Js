import React from "react";

function Settings({ settings, setSettings }) {
  return (
    <div style={{ marginBottom: 20 }}>
      <label>
        Companionâ€™s Name:{" "}
        <input
          value={settings.name}
          onChange={(e) => setSettings({ ...settings, name: e.target.value })}
        />
      </label>
    </div>
  );
}

export default Settings;
